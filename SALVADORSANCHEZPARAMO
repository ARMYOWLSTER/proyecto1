void drawP2()
{
  //////////////////////////////////////////////////////////// SKELETON ////////////////////////////////////////////////
  P2.beginDraw();
  P2.fill(255);
  P2.stroke(2);
  P2.strokeWeight(2);
  P2.beginShape(); 
  P2.fill(#872f37);
  P2.vertex(300, 255);
