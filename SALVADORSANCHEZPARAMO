      fill(255); //// white filll
      text (HP2, 1310, 640);
      text (MP2, 1310, 670);
      popMatrix();
    }
    if ( HP1 <= 0)
    {
      textSize(25);
      text ("PLAYER 1 LOSES", width/2, height/2-200);
      if ( key == ENTER)
      {
        rectMode(CENTER);
        screen = 0;
        HP1= 1500;
        HP2 = 1500;
        sel1= 0;
        sel2= 0;
        turn = 0;
        MP1 = 0;
        MP2 = 0;
      }
    }
    if (HP2 <= 0)
    {
      textSize(25);
      text ("PLAYER 2 LOSES", width/2, height/2-200);
      if ( key == ENTER)
      {
        rectMode(CENTER);
        screen = 0;
        HP1= 1500;
        HP2 = 1500;
        sel1= 0;
        sel2= 0;
        turn = 0;
        MP1 = 0;
        MP2 = 0;
      }
    }
  }
}

