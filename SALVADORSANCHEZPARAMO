  P2.bezierVertex(351, 289, 347, 304, 347, 304);
  P2.vertex(337, 312);
  P2.vertex(340, 289);
  P2.vertex(333, 257);
  P2.vertex(307, 226);
  P2.vertex(283, 208);
  P2.endShape();
  P2.beginShape(); 
  P2.fill(#3d1313);
  P2.vertex(518, 465);
